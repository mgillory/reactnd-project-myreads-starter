{
  "extends": "airbnb",
  "rules": {
    "no-restricted-syntax": 0,
    "no-nested-ternary": 0,
    "no-multiple-empty-lines": [
      2,
      {
        "max": 2,
        "maxEOF": 1
      }
    ],
    "prefer-template": 0,
    "react/prop-types": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "react/no-array-index-key": 0,
    "jsx-a11y/href-no-hash": "off",
    "class-methods-use-this": "off",
    "no-multi-assign": "off",
    "max-len": "off",
    "linebreak-style": "off",
    "quote-props": [
      2,
      "consistent"
    ],
    "import/no-cycle": 0
  }
}